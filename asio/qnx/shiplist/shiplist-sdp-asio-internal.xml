<?xml version="1.0" encoding="UTF-8"?>
<shiplist>
	<id>com.qnx.${baselineId}.osr.asio.internal</id>
	<name>Asio Library</name>
	<description>Asio is a C++ library for network and low-level I/O programming</description>
	<property key="f.support" value="Custom"/>
	<property key="f.swtype" value="III"/>
	<property key="f.engage" value="B"/>
	<category>OSR</category>

	<property key="com.qnx.internal" value="true"/>
	<!-- don't generate provides/requires tags for internal packages-->
	<requires value="suppress-library: autogen"/>
	<provides value="suppress-library: autogen"/>

	<asset dest="${targetfolder}/usr/local/include/asio.hpp" runtime="false" />
	<asset dest="${targetfolder}/usr/local/include/asio/" runtime="false" />
	<asset dest="${targetfolder}/[ARCH:aarch64le,x86_64]/usr/local/lib/pkgconfig/asio.pc" runtime="false" />
</shiplist>
